Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
   "Corrected", " ", "Quartic", " ", "Exponential", " ", "Orthogonal", " ", 
    "Basis", " ", "Implementation"}], "*)"}], 
  RowBox[{"(*", 
   RowBox[{"Define", " ", "the", " ", "weight", " ", "function"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"weight", "[", "x_", "]"}], ":=", 
     RowBox[{"Exp", "[", 
      RowBox[{"-", 
       RowBox[{"x", "^", "4"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Define", " ", "inner", " ", "product", " ", "function", " ", "for", " ", 
     "current", " ", "calculation"}], "*)"}], 
   RowBox[{
    RowBox[{
     RowBox[{"innerProductFunc", "[", 
      RowBox[{"f_", ",", "g_"}], "]"}], ":=", 
     RowBox[{"NIntegrate", "[", 
      RowBox[{
       RowBox[{"f", " ", "g", " ", 
        RowBox[{"weight", "[", "x", "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"-", "Infinity"}], ",", "Infinity"}], "}"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
     "Generate", " ", "orthogonal", " ", "polynomials", " ", "directly", " ", 
      "using", " ", "Gram"}], "-", "Schmidt"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{
     "GramSchmidtQuarticOrthogonalPolynomials", "[", "n_Integer", "]"}], ":=", 
     RowBox[{"Module", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "polys", ",", "monomials", ",", "temp", ",", "prod", ",", "norm", ",",
          "norms", ",", " ", "k", ",", "j"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"monomials", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"x", "^", "i"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"polys", "=", 
         RowBox[{"{", "1", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"norms", "=", 
         RowBox[{"{", 
          RowBox[{"innerProductFunc", "[", 
           RowBox[{"1", ",", "1"}], "]"}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Monitor", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"k", "=", "1"}], ",", 
            RowBox[{"k", "<=", "n"}], ",", 
            RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"temp", "=", 
              RowBox[{"monomials", "[", 
               RowBox[{"[", 
                RowBox[{"k", "+", "1"}], "]"}], "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"For", "[", 
              RowBox[{
               RowBox[{"j", "=", "1"}], ",", 
               RowBox[{"j", "<=", "k"}], ",", 
               RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"prod", "=", 
                 RowBox[{"innerProductFunc", "[", 
                  RowBox[{"temp", ",", 
                   RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], ";", 
                "\[IndentingNewLine]", 
                RowBox[{"norm", "=", 
                 RowBox[{"norms", "[", 
                  RowBox[{"[", "j", "]"}], "]"}]}], ";", 
                "\[IndentingNewLine]", 
                RowBox[{"temp", "=", 
                 RowBox[{"temp", "-", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{"prod", "/", "norm"}], ")"}], "*", 
                   RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}]}]}], ";"}]}], 
              "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"norms", ",", 
               RowBox[{"innerProductFunc", "[", 
                RowBox[{"temp", ",", "temp"}], "]"}]}], "]"}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"AppendTo", "[", 
              RowBox[{"polys", ",", 
               RowBox[{"Simplify", "[", "temp", "]"}]}], "]"}], ";"}]}], 
           "\[IndentingNewLine]", "]"}], ",", 
          RowBox[{"Row", "[", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
            "k", ",", "\"\< / \>\"", ",", "n", ",", "\"\<: \>\"", ",", 
             "\[IndentingNewLine]", 
