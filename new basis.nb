Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"EfficientParallelGramSchmidt", "[", "n_Integer", "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "polys", ",", "monomials", ",", "temp", ",", "proj", ",", "norm", ",", 
       "norms", ",", "k", ",", "j", ",", "threads", ",", "chunks", ",", 
       "chunkSize", ",", "projections"}], "}"}], ",", 
     RowBox[{
      RowBox[{"polys", "=", 
       RowBox[{"{", "1", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"norms", "=", 
       RowBox[{"{", 
        RowBox[{"innerProductFunc", "[", 
         RowBox[{"1", ",", "1"}], "]"}], "}"}]}], ";", "\[IndentingNewLine]", 
      
      RowBox[{"Monitor", "[", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"k", "=", "1"}], ",", 
          RowBox[{"k", "<=", "n"}], ",", 
          RowBox[{"k", "++"}], ",", 
          RowBox[{
           RowBox[{"temp", "=", 
            RowBox[{"monomials", "[", 
             RowBox[{"[", 
              RowBox[{"k", "+", "1"}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"(*", 
            RowBox[{
             RowBox[{"For", " ", "small", " ", "k"}], ",", 
             RowBox[{"compute", " ", "projections", " ", "sequentially"}]}], 
            "*)"}], "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"k", "<=", "3"}], "||", 
              RowBox[{"threads", "==", "1"}]}], ",", "\[IndentingNewLine]", 
             RowBox[{"For", "[", 
              RowBox[{
               RowBox[{"j", "=", "1"}], ",", 
               RowBox[{"j", "<=", "k"}], ",", 
               RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"proj", "=", 
                 RowBox[{"innerProductFunc", "[", 
                  RowBox[{"temp", ",", 
                   RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}]}], ";", 
                "\[IndentingNewLine]", 
                RowBox[{"temp", "=", 
                 RowBox[{"temp", "-", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{"proj", "/", 
                    RowBox[{"norms", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], ")"}], "*", 
                   RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}]}]}]}]}], 
              "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
             RowBox[{"(*", 
              RowBox[{
               RowBox[{"For", " ", "larger", " ", "k"}], ",", 
               RowBox[{
               "split", " ", "projections", " ", "among", " ", "available", " ",
                 "threads"}]}], "*)"}], "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"chunkSize", "=", 
               RowBox[{"Ceiling", "[", 
                RowBox[{"k", "/", "threads"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"chunks", "=", 
               RowBox[{"Partition", "[", 
                RowBox[{
                 RowBox[{"Range", "[", "k", "]"}], ",", "chunkSize", ",", 
                 "chunkSize", ",", "1", ",", 
                 RowBox[{"{", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
              "\[IndentingNewLine]", 
              RowBox[{"(*", 
               RowBox[{
               "Process", " ", "each", " ", "chunk", " ", "in", " ", 
                "parallel"}], "*)"}], 
              RowBox[{"projections", "=", 
               RowBox[{"ParallelTable", "[", 
                RowBox[{
                 RowBox[{"Table", "[", 
                  RowBox[{
                   RowBox[{"innerProductFunc", "[", 
                    RowBox[{"temp", ",", 
                    RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], "]"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"j", ",", "chunk"}], "}"}]}], "]"}], ",", 
                 RowBox[{"{", 
                  RowBox[{"chunk", ",", "chunks"}], "}"}]}], "]"}]}], ";", 
              "\[IndentingNewLine]", "\[IndentingNewLine]", 
              RowBox[{"(*", 
               RowBox[{
               "Apply", " ", "projections", " ", "chunk", " ", "by", " ", 
                "chunk"}], "*)"}], "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
               RowBox[{
                RowBox[{"c", "=", "1"}], ",", 
                RowBox[{"c", "<=", 
                 RowBox[{"Length", "[", "chunks", "]"}]}], ",", 
                RowBox[{"c", "++"}], ",", 
                RowBox[{
                 RowBox[{"chunk", "=", 
                  RowBox[{"chunks", "[", 
                   RowBox[{"[", "c", "]"}], "]"}]}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"For", "[", 
                  RowBox[{
                   RowBox[{"i", "=", "1"}], ",", 
                   RowBox[{"i", "<=", 
                    RowBox[{"Length", "[", "chunk", "]"}]}], ",", 
                   RowBox[{"i", "++"}], ",", 
                   RowBox[{
                    RowBox[{"j", "=", 
                    RowBox[{"chunk", "[", 
                    RowBox[{"[", "i", "]"}], "]"}]}], ";", 
                    "\[IndentingNewLine]", 
                    RowBox[{"temp", "=", 
                    RowBox[{"temp", "-", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"projections", "[", 
                    RowBox[{"[", 
                    RowBox[{"c", ",", "i"}], "]"}], "]"}], "/", 
                    RowBox[{"norms", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}], ")"}], "*", 
                    RowBox[{"polys", "[", 
                    RowBox[{"[", "j", "]"}], "]"}]}]}]}]}]}], "]"}], ";"}]}], 
               "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"(*", 
            RowBox[{
            "Calculate", " ", "norm", " ", "and", " ", "add", " ", "new", " ",
              "orthogonal", " ", "polynomial"}], "*)"}], 
           RowBox[{"norm", "=", 
            RowBox[{"innerProductFunc", "[", 
             RowBox[{"temp", ",", "temp"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"norms", ",", "norm"}], "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"AppendTo", "[", 
            RowBox[{"polys", ",", 
             RowBox[{"Simplify", "[", "temp", "]"}]}], "]"}], ";"}]}], "]"}], 
        ",", 
        RowBox[{"Row", "[", 
         RowBox[{"{", 
          RowBox[{"k", ",", "\"\< / \>\"", ",", "n", ",", "\"\<: \>\"", ",", 
           RowBox[{"ProgressIndicator", "[", 
            RowBox[{"k", ",", 
             RowBox[{"{", 
              RowBox[{"0", ",", 
               RowBox[{"n", "+", "1"}]}], "}"}]}], "]"}]}], "}"}], "]"}]}], 
